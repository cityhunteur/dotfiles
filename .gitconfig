[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[url "git@bitbucket.org:"]
	insteadOf = https://bitbucket.org/
[url "git@github.com:"]
	insteadOf = https://github.com/
[core]
	excludesFile = ~/.gitignore
[pull]
	rebase = true
[user]
	name = Pravin Goomannee
	email = 5994387+Cityhunteur@users.noreply.github.com
[init]
	defaultBranch = main
[color]
	ui = true
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[color "diff"]
	meta = 11
	frag = magenta bold
	func = 146 bold
	commit = yellow bold
	old = red bold
	new = green bold
	whitespace = red reverse
[commit]
	gpgSign = false
[tag]
	forceSignAnnotated = false
	gpgsign = false
[diff]
	tool = difftastic
[difftool]
	prompt = false
[difftool "difftastic"]
	cmd = difft "$LOCAL" "$REMOTE"
[pager]
	difftool = true
[alias]
	dlog = "!f() { : git log ; GIT_EXTERNAL_DIFF=difft git log -p --ext-diff $@; }; f"
	dft = difftool
[url "git@hf.co/"]
	insteadOf = https://hf.co/
[url "git@silverdoor:silverdoorapartments/"]
	insteadOf = https://github.com/silverdoorapartments/
[url "git@resolution:resolutionlifegroup/"]
	insteadOf = https://github.com/resolutionlifegroup/
[credential "https://github.com"]
	helper =
	helper = !/opt/homebrew/bin/gh auth git-credential
[credential "https://gist.github.com"]
	helper =
	helper = !/opt/homebrew/bin/gh auth git-credential
